{
	"daemon": {
		"protocol": "http",
		"host": "localhost",
		"port": 8080,
		"allow_debug": false
	},
	"receivers": {
		"insecure": {
			"name": "Insecure"
		},
		"github": {
			"name": "GitHub",
			"secret": "s33kret",
			"ref": "refs/heads/master"
		},
		"slack": {
			"name": "Slack"
		}		
	},	
	"transformations": {
		"null": {
			"name": "Null"
		},
		"chicken": {
			"name": "Chicken",
			"language": "zxx",
			"clucking": false
		},
		"clucking": {
			"name": "Chicken",
			"language": "eng",
			"clucking": true
		},
		"slack": {
			"name": "SlackText"
		},
		"commits": {
			"name": "GitHubCommits",
			"exclude_additions": true,
			"exclude_modifications": true,
			"exclude_deletions": true
		}				
		
	},
	"dispatchers": {
		"null": {
			"name": "Null"
		},
		"log": {
			"name": "Log"
		},				
		"pubsub": {
			"name": "PubSub",
			"host": "localhost",
			"port": 6379,
			"channel": "webhookd"
		},
		"slack": {
			"name": "Slack",
			"config": "/path/to/.slackcat.conf"
		}
	},
	"webhooks": [
		{
			"endpoint": "/github-test",
			"receiver": "github",
			"dispatchers": [ "pubsub" ]
		},
		{
			"endpoint": "/insecure-test",
		 	"receiver": "insecure",
			"dispatchers": [ "pubsub" ]
		},
		{
			"endpoint": "/slack-test",
			"receiver": "slack",
			"transformations": [ "slack", "chicken" ],
			"dispatchers": [ "slack", "log"]
		}
	]
}
